msgid "Secondary groups"
msgstr "Grupe dytësorë"

#: ../libmatekbd/desktop_mate_peripherals_keyboard_xkb.schemas.in.h:11
msgid "Show flags in the applet"
msgstr "Shfaq flamuj në applet"

